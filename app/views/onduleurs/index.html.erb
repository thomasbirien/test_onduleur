<%= flash[:notice]%>
<table>
  <thead>
    <tr>
      <th>identifier</th>
      <th>date and time</th>
      <th>energy</th>
    </tr>
  </thead>
  <tbody>
    <% @onduleurs.each do |onduleur| %>
    <tr>
      <td><%= onduleur.identifier %></td>
      <td><%= onduleur.datetime %></td>
      <td><%= onduleur.energy %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<div>
<h3>Import the csv file</h3>
  <%=  form_tag import_onduleurs_path, multipart: true do %>
    <%= file_field_tag :file %>
    <%= submit_tag "import CSV" %>
  <% end %>
</div>
